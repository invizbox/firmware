config dnsmasq 'invizbox'
	option authoritative '1'
	option domain 'lan'
	option domainneeded '1'
	option expandhosts '1'
	list interface 'loopback'
	option local '/lan/'
	option localise_queries '1'
	option localservice '1'
	option localuse '1'
	option noresolv '1'
	option readethers '1'
	list rebind_domain 'onion'
	option rebind_localhost '1'
	list server '/onion/172.31.1.1#9053'
	list server '208.67.222.222'
	list server '208.67.220.220'

config dnsmasq 'vpn1'
	option authoritative '1'
	option dhcpscript '/usr/lib/lua/new_device.lua'
	option disabled '1'
	option domain 'lan'
	option domainneeded '1'
	option expandhosts '1'
	list interface 'lan'
	option leasefile '/tmp/dhcp.leases'
	option local '/lan/'
	option localise_queries '1'
	option localservice '1'
	option localuse '0'
	option noresolv '1'
	list notinterface 'loopback'
	option readethers '1'
	option rebind_localhost '1'
	option rebind_protection '1'
	list server '208.67.222.222@tun1'
	list server '208.67.220.220@tun1'

config dnsmasq 'tor'
	option authoritative '1'
	option dhcpscript '/usr/lib/lua/new_device.lua'
	option disabled '1'
	option domain 'lan'
	option domainneeded '1'
	option expandhosts '1'
	list interface 'lan'
	option leasefile '/tmp/dhcp.leases'
	option local '/lan/'
	option localise_queries '1'
	option localservice '1'
	option localuse '0'
	option noresolv '1'
	list notinterface 'loopback'
	option readethers '1'
	list rebind_domain 'onion'
	option rebind_localhost '1'
	option rebind_protection '0'
	list server '172.31.1.1#9053'
	list server '/onion/172.31.1.1#9053'

config dnsmasq 'auto'
	option authoritative '1'
	option dhcpscript '/usr/lib/lua/new_device.lua'
	option disabled '1'
	option domain 'lan'
	option domainneeded '1'
	option expandhosts '1'
	list interface 'lan'
	option leasefile '/tmp/dhcp.leases'
	option local '/lan/'
	option localise_queries '1'
	option localservice '1'
	option localuse '0'
	option noresolv '1'
	list notinterface 'loopback'
	option readethers '1'
	option rebind_localhost '1'
	option rebind_protection '0'
	list server '208.67.222.222'
	list server '208.67.220.220'

config dnsmasq 'captive'
	list address '/#/10.101.0.1'
	option authoritative '1'
	option dhcpscript '/usr/lib/lua/new_device.lua'
	option disabled '0'
	option domain 'lan'
	option domainneeded '1'
	option expandhosts '1'
	list interface 'lan'
	option leasefile '/tmp/dhcp.leases'
	option local '/lan/'
	option localise_queries '1'
	option localservice '1'
	option localuse '0'
	option noresolv '1'
	list notinterface 'loopback'
	option readethers '1'
	option rebind_localhost '1'
	option rebind_protection '1'

config dhcp lan
	option disabled '0'
	option force '1'
	option instance 'captive'
	option interface 'lan'
	option leasetime '10m'
	option limit 251
	option start '1'
