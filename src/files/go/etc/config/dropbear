config dropbear 'lan_vpn1'
	option enable '0'
	option Interface 'lan_vpn1'
	option PasswordAuth 'on'
	option Port '22'
	option RootPasswordAuth 'on'

config dropbear 'lan_tor'
	option enable '0'
	option Interface 'lan_tor'
	option PasswordAuth 'on'
	option Port '22'
	option RootPasswordAuth 'on'

config dropbear 'lan_clear1'
	option enable '0'
	option Interface 'lan_clear1'
	option PasswordAuth 'on'
	option Port '22'
	option RootPasswordAuth 'on'
