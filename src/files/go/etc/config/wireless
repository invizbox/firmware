config wifi-device 'radio0'
	option channel '11'
	option disabled '0'
	option htmode 'HT20'
	option hwmode '11n'
	option path '1e140000.pcie/pci0000:00/0000:00:00.0/0000:01:00.0'
	option type 'mac80211'

config wifi-iface 'wan'
	option device 'radio0'
	option isolate '0'
	option mode 'scan'
	option network 'wan'

config wifi-iface 'lan'
	option device 'radio0'
	option disabled '0'
	option encryption 'psk-mixed'
	option key 'TOKENPASSWORD'
	option mode 'ap'
	option network 'lan_vpn1'
	option ssid 'InvizBoxGo'

config wifi-iface 'ap_tor'
	option device 'radio0'
	option disabled '1'
	option encryption 'psk-mixed'
	option key 'TOKENPASSWORD'
	option mode 'ap'
	option network 'lan_tor'
	option ssid ''

config wifi-iface 'ap_clear1'
	option device 'radio0'
	option disabled '1'
	option encryption 'psk-mixed'
	option key 'TOKENPASSWORD'
	option mode 'ap'
	option network 'lan_clear1'
	option ssid ''
