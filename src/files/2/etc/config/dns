config servers 'cloudflare'
	option name 'Cloudflare'
	list dns_server '1.1.1.1'
	list dns_server '1.0.0.1'

config servers 'google'
	option name 'Google'
	list dns_server '8.8.8.8'
	list dns_server '8.8.4.4'

config servers 'opendns'
	option default 'true'
	option name 'OpenDNS'
	list dns_server '208.67.222.222'
	list dns_server '208.67.220.220'
